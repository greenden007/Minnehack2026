{
  "name": "NitroModules",
  "version": "0.31.10",
  "summary": "Insanely fast native C++, Swift or Kotlin modules with a statically compiled binding layer to JSI.",
  "homepage": "https://github.com/mrousavy/nitro#readme",
  "license": "MIT",
  "authors": "Marc Rousavy <me@mrousavy.com> (https://github.com/mrousavy)",
  "source": {
    "git": "https://github.com/mrousavy/nitro.git",
    "tag": "0.31.10"
  },
  "platforms": {
    "ios": "15.1",
    "visionos": "1.0",
    "osx": "10.13",
    "tvos": "13.4"
  },
  "source_files": [
    "cpp/**/*.{h,hpp}",
    "cpp/**/*.{c,cpp}",
    "ios/**/*.{h,hpp}",
    "ios/**/*.{c,cpp}",
    "ios/**/*.{m,mm}",
    "ios/**/*.swift"
  ],
  "public_header_files": [
    "cpp/core/AnyMap.hpp",
    "cpp/core/ArrayBuffer.hpp",
    "cpp/core/HybridObject.hpp",
    "cpp/core/Null.hpp",
    "cpp/core/Promise.hpp",
    "cpp/entrypoint/HybridNitroModulesProxy.hpp",
    "cpp/entrypoint/InstallNitro.hpp",
    "cpp/registry/HybridObjectRegistry.hpp",
    "cpp/jsi/JSIConverter.hpp",
    "cpp/jsi/JSIHelpers.hpp",
    "cpp/platform/NitroLogger.hpp",
    "cpp/threading/Dispatcher.hpp",
    "cpp/utils/JSCallback.hpp",
    "cpp/utils/FastVectorCopy.hpp",
    "cpp/utils/NitroHash.hpp",
    "cpp/utils/NitroDefines.hpp",
    "cpp/views/CachedProp.hpp",
    "ios/core/ArrayBufferHolder.hpp",
    "ios/core/PromiseHolder.hpp",
    "ios/utils/AnyMapUtils.hpp",
    "ios/utils/AnyMapHolder.hpp",
    "ios/utils/Result.hpp",
    "ios/utils/DateToChronoDate.hpp",
    "ios/utils/RuntimeError.hpp",
    "ios/utils/SwiftClosure.hpp"
  ],
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "SWIFT_OBJC_INTEROP_MODE": "objcxx",
    "DEFINES_MODULE": "YES",
    "HEADER_SEARCH_PATHS": [
      "\"$(PODS_ROOT)/Headers/Private/Yoga\"",
      "$(PODS_ROOT)/glog",
      "$(PODS_ROOT)/boost",
      "$(PODS_ROOT)/DoubleConversion",
      "$(PODS_ROOT)/fast_float/include",
      "$(PODS_ROOT)/fmt/include",
      "$(PODS_ROOT)/SocketRocket",
      "$(PODS_ROOT)/RCT-Folly"
    ],
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED=1 ",
    "OTHER_SWIFT_FLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED"
  },
  "dependencies": {
    "React-jsi": [],
    "React-callinvoker": [],
    "React-Core": [],
    "React-RCTFabric": [],
    "ReactCodegen": [],
    "RCTRequired": [],
    "RCTTypeSafety": [],
    "ReactCommon/turbomodule/bridging": [],
    "ReactCommon/turbomodule/core": [],
    "React-NativeModulesApple": [],
    "Yoga": [],
    "React-Fabric": [],
    "React-graphics": [],
    "React-utils": [],
    "React-featureflags": [],
    "React-debug": [],
    "React-ImageManager": [],
    "React-rendererdebug": [],
    "React-renderercss": [],
    "hermes-engine": [],
    "glog": [],
    "boost": [],
    "DoubleConversion": [],
    "fast_float": [],
    "fmt": [],
    "RCT-Folly": [],
    "SocketRocket": [],
    "RCT-Folly/Fabric": []
  },
  "compiler_flags": "-DRCT_NEW_ARCH_ENABLED=1"
}
