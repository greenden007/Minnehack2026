{
  "name": "RunAnywhereCore",
  "module_name": "RunAnywhereCore",
  "version": "0.18.1",
  "summary": "Core SDK for RunAnywhere React Native - includes RACommons bindings, native bridges, and public API",
  "homepage": "https://runanywhere.com",
  "license": "MIT",
  "authors": "RunAnywhere AI",
  "platforms": {
    "ios": "15.1"
  },
  "source": {
    "git": "https://github.com/RunanywhereAI/sdks.git",
    "tag": "0.18.1"
  },
  "vendored_frameworks": "ios/Binaries/RACommons.xcframework",
  "source_files": [
    "ios/**/*.{swift}",
    "ios/**/*.{h,m,mm}",
    "cpp/HybridRunAnywhereCore.cpp",
    "cpp/HybridRunAnywhereCore.hpp",
    "cpp/bridges/**/*.{cpp,hpp}",
    "nitrogen/generated/shared/**/*.{h,hpp,c,cpp,swift}",
    "nitrogen/generated/ios/**/*.{h,hpp,c,cpp,mm,swift}"
  ],
  "pod_target_xcconfig": {
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20",
    "HEADER_SEARCH_PATHS": [
      "$(PODS_TARGET_SRCROOT)/cpp",
      "$(PODS_TARGET_SRCROOT)/cpp/bridges",
      "$(PODS_TARGET_SRCROOT)/cpp/third_party",
      "$(PODS_TARGET_SRCROOT)/ios/Binaries/RACommons.xcframework/ios-arm64/RACommons.framework/Headers",
      "$(PODS_TARGET_SRCROOT)/ios/Binaries/RACommons.xcframework/ios-arm64_x86_64-simulator/RACommons.framework/Headers",
      "$(PODS_ROOT)/Headers/Public",
      "\"$(PODS_ROOT)/Headers/Private/Yoga\"",
      "$(PODS_ROOT)/glog",
      "$(PODS_ROOT)/boost",
      "$(PODS_ROOT)/DoubleConversion",
      "$(PODS_ROOT)/fast_float/include",
      "$(PODS_ROOT)/fmt/include",
      "$(PODS_ROOT)/SocketRocket",
      "$(PODS_ROOT)/RCT-Folly"
    ],
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) HAS_RACOMMONS=1",
    "DEFINES_MODULE": "YES",
    "SWIFT_OBJC_INTEROP_MODE": "objcxx",
    "OTHER_CPLUSPLUSFLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED=1 ",
    "OTHER_SWIFT_FLAGS": "$(inherited) -DRCT_NEW_ARCH_ENABLED"
  },
  "libraries": [
    "c++",
    "archive",
    "bz2"
  ],
  "frameworks": [
    "Accelerate",
    "Foundation",
    "CoreML",
    "AudioToolbox"
  ],
  "dependencies": {
    "React-jsi": [

    ],
    "React-callinvoker": [

    ],
    "NitroModules": [

    ],
    "React-Core": [

    ],
    "React-RCTFabric": [

    ],
    "ReactCodegen": [

    ],
    "RCTRequired": [

    ],
    "RCTTypeSafety": [

    ],
    "ReactCommon/turbomodule/bridging": [

    ],
    "ReactCommon/turbomodule/core": [

    ],
    "React-NativeModulesApple": [

    ],
    "Yoga": [

    ],
    "React-Fabric": [

    ],
    "React-graphics": [

    ],
    "React-utils": [

    ],
    "React-featureflags": [

    ],
    "React-debug": [

    ],
    "React-ImageManager": [

    ],
    "React-rendererdebug": [

    ],
    "React-renderercss": [

    ],
    "hermes-engine": [

    ],
    "glog": [

    ],
    "boost": [

    ],
    "DoubleConversion": [

    ],
    "fast_float": [

    ],
    "fmt": [

    ],
    "RCT-Folly": [

    ],
    "SocketRocket": [

    ],
    "RCT-Folly/Fabric": [

    ]
  },
  "public_header_files": [
    "nitrogen/generated/shared/**/*.{h,hpp}",
    "nitrogen/generated/ios/RunAnywhereCore-Swift-Cxx-Bridge.hpp"
  ],
  "private_header_files": [
    "nitrogen/generated/ios/c++/**/*.{h,hpp}",
    "nitrogen/generated/shared/**/views/**/*"
  ],
  "compiler_flags": "-DRCT_NEW_ARCH_ENABLED=1"
}
